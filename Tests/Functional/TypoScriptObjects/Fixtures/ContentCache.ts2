prototype(TYPO3.TypoScript:Array).@class = 'TYPO3\\TypoScript\\TypoScriptObjects\\ArrayImplementation'

contentCache.cachedSegment = TYPO3.TypoScript:Array {
	5 = 'Cached segment|'

	10 = ${object.value}

	@cache {
		mode = 'cached'
	}
}

contentCache.nestedCacheSegments = TYPO3.TypoScript:Array {
	5 = 'Outer segment|'

	10 = ${'site=' + site}

	15 = '|'

	20 = TYPO3.TypoScript:Array {

		5 = 'Inner segment|'

		10 = ${'object=' + object.value}

		15 = '|End inner'

		@cache {
			mode = 'cached'

			entryIdentifier {
				object = ${object}
			}
		}
	}

	25 = '|End outer'

	@cache {
		mode = 'cached'

		entryIdentifier {
			site = ${site}
		}
	}
}

contentCache.uncachedSegmentOnTopLevel = TYPO3.TypoScript:Array {
	5 = 'Uncached segment|'

	10 = ${'counter=' + object.counter}

	15 = '|End uncached'

	@cache {
		mode = 'uncached'

		context {
			1 = 'object'
		}
	}
}

contentCache.uncachedSegmentInCachedSegment = TYPO3.TypoScript:Array {
	5 = 'Outer segment|'

	10 = ${'object=' + object.value}

	15 = '|'

	20 =  TYPO3.TypoScript:Array {
		5 = 'Uncached segment|'

		10 = ${'counter=' + object.counter}

		15 = '|End uncached'

		@cache {
			mode = 'uncached'

			context {
				1 = 'object'
			}
		}
	}

	25 = '|End outer'

	@cache {
		mode = 'cached'

		entryIdentifier {
			object = ${object}
		}
	}
}

contentCache.cacheSegmentsWithTags = TYPO3.TypoScript:Array {
	5 = 'Outer segment|'

	10 = ${'counter=' + object.counter}

	15 = '|'

	20 = TYPO3.TypoScript:Array {

		5 = 'Inner segment 1|'

		10 = ${'object=' + object.value}

		15 = '|End inner'

		@cache {
			mode = 'cached'

			entryIdentifier {
				object = ${object}
			}

			entryTags {
				1 = ${'Object_' + object.id}
				2 = 'NodeType_Acme.Demo:SampleNodeType'
			}
		}
	}

	25 = TYPO3.TypoScript:Array {

		5 = 'Inner segment 2|'

		10 = ${'object=' + object.value}

		15 = '|End inner'

		@cache {
			mode = 'cached'

			entryIdentifier {
				object = ${object}
			}

			entryTags {
				1 = 'Never'
			}
		}
	}

	30 = '|End outer'

	@cache {
		mode = 'cached'

		entryIdentifier {
			object = ${object}
		}

		entryTags {
			1 = 'Outer'
		}
	}
}

contentCache.cachedSegmentWithProcessor = TYPO3.TypoScript:Array {

	5 = 'Cached segment|'

	10 = ${'object=' + object.value}

	15 = '|End cached'

	@process.wrap = ${'Processor start|counter=' + object.counter + '|' + value + '|Processor end'}

	@cache {
		mode = 'cached'

		entryIdentifier {
			object = ${object}
		}
	}

}

contentCache.uncachedSegmentWithProcessor = TYPO3.TypoScript:Array {

	5 = 'Cached segment|'

	10 = TYPO3.TypoScript:Array {
		5 = 'Uncached segment|'

		10 = ${'object=' + object.value}

		15 = '|End cached'

		@process.wrap = ${'Processor start|counter=' + object.counter + '|' + value + '|Processor end'}

		@cache {
			mode = 'uncached'

			context {
				1 = 'object'
			}
		}
	}

	15 = '|End segment'

	@cache {
		mode = 'cached'

		entryIdentifier {
			object = ${object}
		}
	}

}

contentCache.nestedCacheSegmentsWithInnerException = TYPO3.TypoScript:Array {

	5 = 'Cached segment|'

	10 = ${'counter=' + object.counter + '|'}

	15 = TYPO3.TypoScript:Collection {
		collection = ${['Foo', 'Bar']}
		itemName = 'item'
		itemRenderer = TYPO3.TypoScript:Template {
			templatePath = 'no.such.file.here.so.crash.please'

			@cache {
				mode = 'cached'

				entryIdentifier {
					item = ${item}
				}
			}

			@exceptionHandler = 'TYPO3\\TypoScript\\Core\\ExceptionHandlers\\PlainTextHandler'
		}
	}

	20 = '|End segment'

	@cache {
		mode = 'cached'

		entryIdentifier {
			object = ${object}
		}
	}

}
